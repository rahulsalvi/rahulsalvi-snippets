{
    "foreach loop": {
        "prefix": "foreach",
        "body": [
            "for (${1:auto} ${2:i} : ${3:container}){",
            "    $0",
            "}"
        ],
        "description": "foreach loop"
    },
    "class": {
        "prefix": "class",
        "body": [
            "class ${1:name} {",
            "    public:",
            "        $1();",
            "        ~$1();",
            "    private:",
            "        $0",
            "};"
        ],
        "description": "class"
    },
    "namespace": {
        "prefix": "namespace",
        "body": [
            "namespace ${1:name} {",
            "    $0",
            "}"
        ],
        "description": "namespace"
    },
    "static cast": {
        "prefix": "sca",
        "body": [
            "static_cast<${1:type}>(${2:expression})"
        ],
        "description": "static cast"
    },
    "dynamic cast": {
        "prefix": "dca",
        "body": [
            "dynamic_cast<${1:type}>(${2:expression})"
        ],
        "description": "dynamic cast"
    },
    "reinterpret cast": {
        "prefix": "rca",
        "body": [
            "reinterpret_cast<${1:type}>(${2:expression})"
        ],
        "description": "reinterpret cast"
    },
    "const cast": {
        "prefix": "cca",
        "body": [
            "const_cast<${1:type}>(${2:expression})"
        ],
        "description": "const cast"
    }
}
